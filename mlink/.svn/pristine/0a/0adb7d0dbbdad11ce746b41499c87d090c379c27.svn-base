<{include file="b_block_header.html"}>
<{include file="b_block_banner.html"}>

<div>
    <div class="container" style="margin-top:30px;width:100%">
        <div style="text-align:center;margin-bottom:30px;"><h1><{$title}></h1></div>
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">Search</div>
                    <div class="panel-body" >
                        <form>
                            <!--<input type="hidden" name="p" value="1" />-->
                            <div class="col-lg-12 form-inline">
                                <div class="form-group">
                                    Name:&nbsp;<input type="text" Name="name" class="form-control" id="Name"  placeholder="Name">
                                    Modified person:&nbsp;<input type="text" Name="updateuser" class="form-control" id="Account"  placeholder="Account">
                                    <!--Status:&nbsp;<select class="form-control" id='Status' name="status">-->
                                    <!--<option value="All">All</option>-->
                                    <!--<option value="0">To modify</option>-->
                                    <!--<option value="1">To apply for modifying</option>-->
                                    <!--<option value="2">revocation</option>-->
                                <!--</select>-->
                                    <div class="form-group" style="margin-left:20px">
                                        <button type="submit" class="btn  btn-primary Searchpub">Search</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>

            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">Result</div>
                    <div class="panel-body">
                        <table class="table table-striped">
                            <thead>
                            <tr>
                                <th>Number</th>
                                <th>Name</th>
                                <th>account</th>
                                <th>Application time / Update time</th>
                               <!-- <th>Status</th> -->
                                <th>Modified person</th>
                                <th>Change Log</th>
                                <th>detail</th>
                            </tr>
                            </thead>
                            <{foreach from=$list item=publisher}>
                            <tr  class="open-logs">
                                <td><{$publisher.Number}></td>
                                <td><{$publisher.Name}></td>
                                <td><{$publisher.UserName}></td>
                                <td><{$publisher.time}></td>
                              <!--  <td>
                                    <{if $publisher.state eq '0'}>
                                    To apply for modifying
                                    <{elseif $publisher.state eq '2'}>
                                    revocation
                                    <{else}>
                                    To modify
                                    <{/if}>
                                </td> -->
                                <td>
                                    <{if $publisher.update_user eq ''}>
                                    Null
                                    <{else}>
                                    <{$publisher.update_user}>
                                    <{/if}>
                                </td>
                                <td>
                                    <a href="b_publisher_changelog.php?id=<{$publisher.ID}>&name=<{$publisher.Name}>" target="_blank" style="cursor: pointer">Change Log</a>
                                </td>
                                <td>
                                    <{if $publisher.uptype eq '0'}>
                                    <a id="detail" href="b_publisher_edit.php?id=<{$publisher.ID}>&type=2" style="cursor: pointer" target="_log">Detail</a>
                                    <{else}>
                                    <a class="noopen" href="javascript:void(0);" style="cursor: pointer"><font color="gray">Detail</font></a>
                                    <{/if}>
                                </td>
                            </tr>
                            <{/foreach}>
                        </table>
                        <{$pageHtml}>
                    </div>
                    <input type="hidden" value="<{$url}>" id="url">
                </div>

            </div>
        </div>
    </div>
    <script>
        $('.noopen').click(function(){
            alert('Has been modified to complete');
        });
    </script>
    <{include file="b_block_footer.html"}>