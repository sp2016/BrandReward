/* Global Styles */
body, p, div, td, th, span {
  font-size: small;
  font-family: "Open Sans",sans-serif;
}

body, p, td, th, h1, h2, h3, h4, h5 {
  color: #4d5051;
}

th, b, strong {
  font-weight: 600;
}

h1, ul, li {
  margin:0;
  padding:0;
}

h1 {
  background-image: none;
  font-size: 1.7em;
}

h2 {
  background-image: none;
  font-size: 1.4em;
  border-bottom: 0;
}

h3 {
  background-image: none;
  font-size: 1.2em;
}

h4 {
  background-image: none;
  font-size: 1em;
}

h5 {
  background-image: none;
  font-size: .8em;
}

h1, h2, h3, h4, h5 {
  line-height: 2em;
  margin: 0;
}

/* Default Links */

a {
  cursor: pointer;
  color: #088295;
  text-decoration: none;
}

a:hover {
  color: #088295;
  text-decoration: underline;
}

.l1, b > a {
  cursor: pointer;
  color: #585858;
  text-decoration: none;
  font-weight: 600;
  text-shadow: 0 1px 1px #DFDFDF;
}

.l1:hover, b > a:hover {
    color: #585858;
    text-decoration: underline;
}

.l2 {
  cursor: pointer;
  color: #585858;
  text-decoration: none;
  text-transform: uppercase;
  text-shadow: 0 1px 1px #DFDFDF;
  font-weight: normal;
}

.l2 span {
    color: #585858;
}

.l2:hover, .l2 span:hover, .l3:hover {
  color: #088295;
  text-decoration: none;
}

.l2 [class*="i-cj"]:before {
  font-size: 1.2em;
}

.l2.disabled, .l2.disabled span, .l2.disabled span:hover{
    cursor: default;
    color: #B8B8B8;
}

.l3, .l3 span {
  cursor: pointer;
  color: #B8B8B8;
  text-decoration: none;
  text-transform: uppercase;
}

.l3 [class*="i-cj"]:before {
  color: #B8B8B8;
  font-size: 1.5em;
  font-weight: 600;
  position: relative;
  top: 4px;
}

.l3:hover, .l3 span:hover, .l3 [class*="i-cj"]:hover {
  color: #088295;
  text-decoration: none;
}

.l4 [class*="i-cj"]:before, .l3 [class*="i-cj"]:before, .l2 [class*="i-cj"]:before {
  margin-right: .3em;
}

.l4 {
  cursor: pointer;
  color: #858585;
  font-size: 1.5em;
}

.l4:hover, .l4 [class*="i-cj"]:hover {
  color: #088295;
  text-decoration: none;
}

/* Basic tab links */

.nav-links {
  margin-bottom: 0;
}

.nav-links li a {
  font-size: 1.1em;
  text-shadow: 0 1px 1px #DFDFDF;
  text-decoration: none;
  padding: 0 0 0 12px;
  margin: 2px 0;
  color: #B8B8B8;
  text-transform: uppercase;
}

.nav-links li a:hover{
  color: #088295;
  text-decoration: none;
  background-color: rgba(0, 0, 0, 0);
}

.nav-links .active a {
  color: #585858;
}

.nav-links li {
  display: inline-block;
}

.nav-links li a:after {
  content: "|";
  padding-left: 12px;
  color: #D1D1D1;
  text-shadow: none;
}

.nav-links li:last-child a:after, .nav-links li:last-child span a:after {
  content: none;
}

/* Floats */

.fl-right {
  float: right;
}

.fl-left {
  float: left;
}

/* Content */

ul.inline > li, ol.inline > li {
  display: inline-block;
  padding-right: 5px;
  padding-left: 5px;
}

.horizontal-separator {
  margin: 20px 0;
  border: 0;
  border-top: 1px solid #EEE;
  border-bottom: 1px solid #FFF;
}

.widget-content > .horizontal-separator {
  padding: 0;
}

/* Lists spacing */

.spaced > li {
  margin-top: 9px;
  margin-bottom: 9px;
}

.spaced2 > li {
  margin-top: 15px;
  margin-bottom: 15px;
}

/* Input styles */

textarea, input[type="file"], input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {
  display: inline-block;
/*  height: 2.3em;*/
  padding: 4px 6px;
  margin-bottom: 9px;
  font-size: 12px;
  line-height: 20px;
}

/* Main Layout */
.content {
  padding: 0 1em;
}

#contentContainer {
  padding-top: 1em;
}

.content-container-main {
  background: #FFF;
  margin-top: 5px;
}

.content-space {
  margin: 2em 0;
}

/* Breadcrumbs */
.page-history {
  margin-left: 14px;
  padding: 10px 0;
}

.page-history a {
  font-size: 1.1em;
  cursor: pointer;
}

.page-history span {
  margin-left: 8px;
  margin-right: 8px;
  color: #ACACAC;
}

.page-history label {
  display: inline-block;
  color: #858585;
  font-size: 1.1em;
}

/* CSS tab - Widget Headings & Content */

.widget-title {
  margin: 10px 0 30px 0;
  height: 17px;
  border-bottom: 4px solid #585858;
}

.widget-title-text {
  margin: 0;
  padding: 4px 15px 5px 0;
  height: 30px;
  line-height: 30px;
  background: #FFF;
  color: #585858;
  font-size: 1.2em;
  font-weight: 600;
  white-space: nowrap;
  display: inline-block;
}

.widget-header-title-text {
  margin: 0;
  -webkit-text-shadow: 0 2px 1px white;
  -moz-text-shadow: 0 2px 1px white;
  text-shadow: 0 2px 1px white;
  font-size: 1em;
  font-weight: 600;
  line-height: 1em;
}

/* Sub-headings */

.sub-widget-title {
  margin-bottom: 10px;
  height: 17px;
  border-bottom: 2px solid #949494;
}

.sub-widget-title-text {
  padding: 4px 14px 5px 0;
  height: 30px;
  background-color: white;
  color: #808285;
  font-weight: normal;
  font-size: small;
  line-height: 34px;
  display: inline;
}

.br-header {
  border-bottom: 1px solid #EEE;
  padding-bottom: 4px;
  margin: 2em 0 .5em;
}

.br-header > [class*="icon-"] {
  margin-right: 4px;
}

/* Body Copy */

.lead-body {
  color: #E2E2E2;
  font-size: 2.5em;
  line-height: 40px;
  font-weight: 200;
}

/* List Styles */

.br-uo-list > li {
  list-style: disc;
  list-style-position: outside;
}

.br-uo-list ul li {
  list-style-type: circle;
}

.br-uo-list, .br-uo-list ul, .br-o-list, .br-o-list ol {
  margin-left: 40px;
}

.br-uo-list ul {
  margin-bottom:0.5em;
  margin-top:0.5em;
}

.br-o-list li {
  list-style-type: decimal;
}

/* Basic widget container styles */

.widget-header {
  background: #FFF;
  background: -moz-linear-gradient(top, #FFF 0%, #F9F9F9 10%, #F9F9F9 22%, #FFF 84%);
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#FFF), color-stop(10%,#F9F9F9), color-stop(22%,#F9F9F9), color-stop(84%,#FFF));
  background: -webkit-linear-gradient(top, #FFF 0%,#F9F9F9 10%,#F9F9F9 22%,#FFF 84%);
  background: -o-linear-gradient(top, #FFF 0%,#F9F9F9 10%,#F9F9F9 22%,#FFF 84%);
  background: -ms-linear-gradient(top, #FFF 0%,#F9F9F9 10%,#F9F9F9 22%,#FFF 84%);
  background: linear-gradient(to bottom, #FFF 0%,#F9F9F9 10%,#F9F9F9 22%,#FFF 84%);
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ffffff',GradientType=0 );
  border: 1px solid #E9E9E9;
  padding: 10px;
  color: #B8B8B8;
  text-shadow: 0 1px 2px #FFF;
}

.widget-container {
  margin: 0px 10px 40px 10px;
}

.widget-content {
  background-color: #FFF;
  border: 1px solid #E6E5E3;
  min-height: 100px;
  margin-bottom: 20px;
}

.default-content-container > div {
  padding: 1em;
}

.widget-actions-bar {
  padding: 1em;
  border-bottom: 1px solid #E6E5E3;
}

.widget-title-icon {
  font-size: 3em;
  color: #585858;
  text-shadow: 0 1px 1px #AFAFAF;
  float: left;
}

.widget-title-icon-circle {
  height: 30px;
  width: 30px;
  float: left;
  margin-right: 20px;
}

.widget-title-icon-and-text {
  margin-top: 5px;
  margin-bottom: 5px;
  height: 40px;
  line-height: 40px;
  background: #FFF;
  color: #585858;
  font-size: 1.2em;
  font-weight: 600;
  white-space: nowrap;
}

.widget-title-icon-and-text.example-header {
  width: 250px;
}

/* Basic Link Row */

.simple-search-row {
  padding: 10px;
  background-color: #FFF;
  border-bottom: 1px dashed #CCC;
  height: 75px;
}

.simple-search-row:hover {
  background: #F9F9F9;
}

.link-preview {
  margin-right: 15px;
  width: 60px;
  height: 60px;
  margin-left: 8px;
  font-size: .8em;
  text-align: center;
}

.link-title {
  max-width: 50em;
  margin-top: .2em;
  width: 30%;
  line-height: 1.3;
}

.fl {
  float: left;
}

.fl-r {
  float: right;
}

.link-title div {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 100%;
}

.link-name {
  display: block;
  font-size: 1.2em;
  font-weight: 600;
  color: #3D3D3D;
  text-shadow: 0 1px 1px #EFEFEF;
}

/* Alerts & Errors */

.alert-basic {
  color: #FFB500;
  background-color: rgb(223, 233, 235);
  border-color: #FFB500;
  text-align: center;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
}

.alert-error {
  color: #E1523D;
  background-color: rgb(225, 82, 61);
  border-color: #E1523D;
  text-align: center;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
}

.alert-success {
  color: #49C5B1;
  background-color: rgb(228, 247, 243);
  border-color: #49C5B1;
  text-align: center;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
}

.alert-info {
  color: #34657F;
  background-color: rgb(223, 233, 235);
  border-color: #34657F;
  text-align: center;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
}

.alert-warning {
  color: #8a6d3b;
  background-color: rgb(254, 247, 228);
  border-color: #faebcc;
  text-align: center;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
}

.alert-danger {
  color: #a94442;
  background-color: rgb(242, 222, 223);
  border-color: #ebccd1;
  text-align: center;
  text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
}

.alert-danger-error-list {
    list-style-type: none;
}

.alert-error .close {
  color: #E1523D;
}

.alert-success .close {
  color: #49C5B1;
}

.alert-basic .close {
  color: #FFB500;
}

.alert-info .close {
  color: #34657F;
}

.alert-warning .close {
  color: #8a6d3b;
}

.alert-danger .close {
  color: #a94442;
}

.close {
  float: right;
  font-size: 20px;
  font-weight: 600;
  text-shadow: 0 1px 0 #FFF;
  opacity: .5;
}

.close:hover {
  opacity: 1;
}

.alert-basic.block-msg {
  border: 1px solid #FFB500;
  background: #FFB500;
}

.alert-error.block-msg {
  border: 1px solid #E1523D;
  background: #E1523D;
}

.alert-info.block-msg {
  border: 1px solid #34657F;
  background: #34657F;
}

.alert-success.block-msg {
  border: 1px solid #49C5B1;
  background: #49C5B1;
}

.alert-basic.block-msg, .alert-error.block-msg, .alert-info.block-msg, .alert-success.block-msg, .alert-error.block-msg .close, .alert-info.block-msg .close, .alert-success.block-msg .close, .alert-basic.block-msg .close {
  color: #FFF;
  text-shadow: none;
}

/* Default Form Styles */

textarea, input, iframe, input[type="file"], input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {
  border: 1px solid #DDD;
  -webkit-box-shadow: inset 0 0 2px 0 rgba(0, 0, 0, 0.09);
  -moz-box-shadow: inset 0 0 2px 0 rgba(0, 0, 0, 0.09);
  box-shadow: inset 0 0 2px 0 rgba(0, 0, 0, 0.09);
}

select, textarea, input, input[type="file"], input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  color: #6d6e70;
}

textarea:focus, input:focus, input[type="file"]:focus, input[type="text"]:focus, input[type="password"]:focus, input[type="datetime"]:focus, input[type="datetime-local"]:focus, input[type="date"]:focus, input[type="month"]:focus, input[type="time"]:focus, input[type="week"]:focus, input[type="number"]:focus, input[type="email"]:focus, input[type="url"]:focus, input[type="search"]:focus, input[type="tel"]:focus, input[type="color"]:focus, .uneditable-input:focus, .form-control:focus {
  outline: none;
  outline: thin dotted  9;
  border: 1px solid rgba(0, 175, 102, 0.75);
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 4px rgba(0, 175, 102, 0.4);
  -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 4px rgba(0, 175, 102, 0.4);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 4px rgba(0, 175, 102, 0.4);
}

.sizing-block input, .sizing-block select {
  display: block;
}

input[type="checkbox"], input[type="radio"] {
  border: 0;
  box-shadow: 0;
}

/* Custom checkbox and radio styles */

fieldset p {
  position: relative;
  display: block;
  overflow: hidden;
}

.br-form-input {
  margin-bottom: 0;
  position: relative;
}

.br-form-input > input {
  position: absolute;
  left: 0;
  float: left;
  margin: 0;
  padding: 0;
  width: 16px;
  height: 16px!important;
  opacity: 0;
  z-index: 1;
}

.br-form-input > input:hover {
  cursor: pointer;
}

.br-form-input > input:disabled {
  cursor: default;
}

p > label  {
  float: left;
  padding: 0 0 0 18px;
/*  line-height: 16px;*/
}

p:not(#cjinput) > input + label:before {
  background: url(../../../ui-kit/images/br-custom-inputs.png) 0 -1px no-repeat;
  height: 16px;
  width: 16px;
  position: absolute;
  content: "";
  margin-left: -18px;
  z-index: 0;
}

p:not(#cjinput) > input[type=radio] + label:before {
  background-position: 0 -161px;
}

/* Checked styles */
p:not(#cjinput) > input[type=radio]:checked + label:before {
  background-position: 0 -241px;
}

p:not(#cjinput) > input[type=checkbox]:checked + label:before {
  background-position: 0 -81px;
}

p:not(#cjinput) > input[type=checkbox]:hover:checked + label:before,
p:not(#cjinput) > input[type=checkbox]:focus:checked + label:before,
p:not(#cjinput) > input[type=checkbox]:checked + label:before:hover,
p:not(#cjinput) > input[type=checkbox]:focus:checked + label:before {
  background-position: 0 -101px;
}

p:not(#cjinput) > input[type=radio]:hover:checked + label:before ,
p:not(#cjinput) > input[type=radio]:focus:checked + label:before ,
p:not(#cjinput) > input[type=radio]:checked + label:before:hover,
p:not(#cjinput) > input[type=radio]:focus:checked + label:before  {
  background-position: 0 -261px;
}

/* Hover & Focus styles */
p:not(#cjinput) > input[type=checkbox]:hover + label:before,
p:not(#cjinput) > input[type=checkbox]:focus + label:before,
p:not(#cjinput) > input[type=checkbox] + label:before:hover {
  background-position: 0 -21px;
  cursor: pointer;
}

p:not(#cjinput) > input[type=radio]:hover + label:before ,
p:not(#cjinput) > input[type=radio]:focus + label:before ,
p:not(#cjinput) > input[type=radio] + label:before:hover {
  background-position: 0 -181px;
  cursor: pointer;
}

/* Active styles */
p:not(#cjinput) > input[type=checkbox]:active + label:before,
p:not(#cjinput) > input[type=checkbox] + label:before:hover:active {
  background-position: 0 -41px;
}

p:not(#cjinput) > input[type=radio]:active + label:before ,
p:not(#cjinput) > input[type=radio] + label:before:hover:active {
  background-position: 0 -201px;
}

p:not(#cjinput) > input[type=checkbox]:active:checked + label:before,
p:not(#cjinput) > input[type=checkbox]:checked + label:before:hover:active {
  background-position: 0 -121px;
}

p:not(#cjinput) > input[type=radio]:active:checked + label:before ,
p:not(#cjinput) > input[type=radio]:checked + label:before:hover:active {
  background-position: 0 -281px;
}

/* Disabled styles */
p:not(#cjinput) > input[type=checkbox]:disabled + label:before,
p:not(#cjinput) > input[type=checkbox]:hover:disabled + label:before,
p:not(#cjinput) > input[type=checkbox]:focus:disabled + label:before {
  background-position: 0 -61px;
}

p:not(#cjinput) > input[type=radio]:disabled + label:before ,
p:not(#cjinput) > input[type=radio]:hover:disabled + label:before ,
p:not(#cjinput) > input[type=radio]:focus:disabled + label:before {
  background-position: 0 -221px;
}

p:not(#cjinput) > input[type=checkbox]:disabled:checked + label:before,
p:not(#cjinput) > input[type=checkbox]:hover:disabled:checked + label:before,
p:not(#cjinput) > input[type=checkbox]:focus:disabled:checked + label:before {
  background-position: 0 -141px;
}

p:not(#cjinput) > input[type=radio]:disabled:checked + label:before ,
p:not(#cjinput) > input[type=radio]:hover:disabled:checked + label:before ,
p:not(#cjinput) > input[type=radio]:focus:disabled:checked + label:before {
  background-position: 0 -301px;
}

/* Inline Checkbox & inline radio */
.inline-checkbox fieldset p, .inline-radio p, p.inline-checkbox {
  display: inline-block;
}

p.inline-checkbox {
    margin-bottom: 0;
}
p.inline-checkbox label:before {
    margin-bottom: 0;
}

.inline-radio p {
  padding-right: 1em;
}

/* Selects */

select:focus {
  outline: none;
}

/* Form Control States */
input:focus:required:invalid:focus, textarea:focus:required:invalid:focus, select:focus:required:invalid:focus {
  border-color: #E1523D;
  -webkit-box-shadow: 0 0 6px #F8B9B7;
  -moz-box-shadow: 0 0 6px #f8b9b7;
  box-shadow: 0 0 6px #F8B9B7;
}

.form-group input {
  display: inline-block;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  -moz-box-sizing: border-box;
  height: 32px;
  padding: 0 8px;
  border-width: 1px;
  border-style: solid;
  border-radius: 0;
  background: #FFF;
  font: 13px/16px 'Open Sans',Helvetica,Arial,sans-serif;
  color: #404040;
  appearance: normal;
  -moz-appearance: none;
  -webkit-appearance: none;
  margin-bottom: 0; /* Removes margin-bottom for all form-group inputs that is normally there in regular inputs*/
}

.form-horizontal .form-group {
