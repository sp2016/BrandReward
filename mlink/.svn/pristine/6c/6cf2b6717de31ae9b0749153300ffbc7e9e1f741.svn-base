<script>
        $('.checklog').bind('click',function(){
            var id = $(this).data('id');
            var data = {'check':id};
            $.ajax({
                type:"post",
                url:"b_publisher_changelog.php",
                data:data,
                async:false,
                success: function(res){

                    var data = JSON.parse(res);
                    var html = '';
                    var html1 = '';
                    if(data['old']['info'] != ''){
                        html+='<tr>';
                        $.each(data['old']['info'], function(i, item) {
                            if(i == 'Country'){
                                var name = $('.Country').parent().prev().html();
                                var oval = $("#pub_country option[value="+item+"]").text();
                                html+='<td   style="text-align:center;><font color="white">'+name+'</font></td>';
                                html+='<td class="old'+i+'" style="text-align:center;><font color="white">'+oval+'</font></td>';
                                html+=' </tr>';
                            }else{
                                var name = $('.'+i).parent().prev().html();
                                html+='<td  style="text-align:center;><font color="white">'+name+'</font></td>';
                                html+='<td  class="old'+i+'" style="text-align:center;><font color="white">'+item+'</font></td>';
                                html+=' </tr>';
                            }
                        })
                    }
                    if(data['old']['detail'] != ''){
                        html+='<tr>';
                        $.each(data['old']['detail'], function(i, item) {
                            var nval = item;
                            if(i == 'CategoryId'){
                                var arr = nval.split(',');
                                var text = '';
                                var accext = '';
                                var arttext = '';
                                var autext = '';
                                var beatext = '';
                                var boktext = '';
                                var bustext = '';
                                var buytext = '';
                                $.each(arr, function(i, item) {
                                    var oval = $("#subCat_"+item).data('text');
                                    var pval = $("#subCat_"+item).data('id');
                                    var p = $('#priCat_'+pval).data('text');
                                    if(p == 'Accessories'){
                                        var aval = '';
                                        aval+=oval+' , ';
                                        accext+=aval;
                                    }
                                    else if(p == 'Art/Photo/Music'){
                                        var aval = '';
                                        aval+=oval+' , ';
                                        arttext+=aval;
                                    }else if(p == 'Automotive'){
                                        var aval = '';
                                        aval+=oval+' , ';
                                        autext+=aval;
                                    }else if(p == 'Beauty'){
                                        var aval = '';
                                        aval+=oval+' , ';
                                        beatext+=aval;
                                    }else if(p == 'Books/Media'){
                                        var aval = '';
                                        aval+=oval+' , ';
                                        boktext+=aval;
                                    }else if(p == 'Business'){
                                        var aval = '';
                                        aval+=oval+' , ';
                                        bustext+=aval;
                                    }else{
                                        var aval = '';
                                        aval+=oval+' , ';
                                        buytext+=aval;
                                    }
                                });
                                if(accext != ''){
                                    accext = 'Accessories: '+accext.substr(0,accext.length-3)+'<br>'+'<hr style="height:1px;border:none;border-top:1px solid #555555;" />';
                                }
                                if(arttext != ''){
