 <div class="container" style="margin-top:30px;width: 100%;">
        <div class="row" style="padding:20px 0;">
            <div class="col-lg-8">
                <div class="panel panel-default">
                    <div class="panel-heading"><{$LANG.backend.b_merchant_domains.a1}>: <{$search.name}></div>
                    <div class="panel-body">
                        <table class="table table-striped">
                            <thead>
                            <tr>
                                <th style="width:20%"><{$LANG.backend.b_merchant_domains.a2}></th>
                                <th style="width:60%"><{$LANG.backend.b_merchant_domains.a3}></th>
                                <th style="width:20%"><{$LANG.backend.b_merchant_domains.a5}></th>

                            </tr>
                            </thead>
                            <{foreach from=$domains item=domain}>
                            <!-- <{foreach from=$domain.Outbound item=out}>
                            <tr class="store">
                                <td><{$out.1}></td>
                                <td><{$out.0}></td>
                                <td><{if $domain.CommissionUsed == '0'}>other<{else}><{if $domain.CommissionType == 'Value'}><{if $domain.CommissionCurrency != ''}><{$domain.CommissionCurrency}><{else}>USD<{/if}><{$domain.CommissionUsed}><{else}><{$domain.CommissionUsed}>%<{/if}><{/if}></td>
                            </tr>
                            <{/foreach}> -->
                            <tr class="store">
                                <td style="width:20%;vertical-align: middle;"><{$domain.domain}></td>
                                <td style="width:60%;vertical-align: middle;word-wrap: break-word;word-break: break-all;"><{$domain.country}></td>
                                <!-- <td style="width:20%;vertical-align: middle;"><{if $domain.CommissionUsed == '0'}>other<{else}><{if $domain.CommissionType == 'Value'}><{if $domain.CommissionCurrency != ''}><{$domain.CommissionCurrency}><{else}>USD<{/if}><{$domain.CommissionUsed}><{else}><{$domain.CommissionUsed}>%<{/if}><{/if}></td> -->
                                <td style="width:20%;vertical-align: middle;"><{$domain.CommissionRange}></td>
                            </tr>
                            <{/foreach}>
                        </table>
                    </div>
                </div>

            </div>


            <div class="col-lg-4">
                <div class="panel panel-default">
                    <div class="panel-heading" style="font-weight :bold"><{$LANG.backend.b_merchant_domains.a6}></div>
                    <div class="panel-body">
                        <table class="table table-striped">
                            <{foreach from=$pArr item=p}>
                            <tr class="store">
                                <td><{$p}></td>
                            </tr>
                            <{/foreach}>
                        </table>
                    </div>
                </div>

            </div>

        </div>
    </div>
