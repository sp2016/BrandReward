@media screen and (max-width: 1700px){ .container .banner h1{ padding-top:120px }}
@media screen and (max-width: 1600px){ .container .banner h1{ padding-top:100px }}
@media screen and (max-width: 1500px){ .container .banner h1{ padding-top:80px }}
@media screen and (max-width: 1400px){ .container .banner h1{ padding-top:60px } }
@media screen and (max-width: 1300px){
    .container .banner h1{ padding-top:60px; font-size:7em}
    .container .banner h2{ padding: 0; font-size: 2.4em}
    .container .banner .txt{ font-size: 1.5em}
    .container .banner .btn a{ width: 160px}
}
@media screen and (max-width: 1240px){
    .header .main,.container .int,.container ul.fea,.container .pub_con, .ref_con, .part_con,.container .contact,.layer .content,.container .fea_word,.container ul.team_text, .container .team_touch{width: 96%; margin: 0 1em;}
    .header .main ul.menu{ position: absolute; width: 180px; background-color: #fff; border: 1px solid #60738d; padding: 2px 2px; margin: 0; right: 0;
        top: 100px;z-index: 99;display: none}
    .header .main ul.menu li{ width: 100%; text-align: center; margin-bottom: 1px;margin: 0}
    .phone_menu_btn{ background: url(../img/icon.png) -560px -129px no-repeat; }
    .header .main .btn_sl {margin-right: 40px; float: right;}
    .country{ float: left; margin-left: 30px}
    i.nav_d{ display: inline-block; float: left; width: 32px ; height: 32px; background: url(../img/icon.png) no-repeat -546px -114px; float: right;  margin: 50px 0 0 0; cursor: pointer;}
    .container .fea_word{margin: 20px auto;}
    .container .team_touch{ margin: 0 auto 30px}
}
@media screen and (max-width: 1200px){
    .container .banner h1{ font-size:4em}
    .container .banner h2{ font-size: 1.5em; margin-top: 10px}
    .container .banner .txt{ margin-top: 10px; font-size: 1.2em}
    .container .banner .btn{ width: 270px}
    .header .main ul.menu li { font-size: 1.2em; border-bottom: 1px solid #fff}
    .header .main ul.menu li a{ height: 48px; line-height: 48px}
    .container .banner .btn a{ font-size: 1.2em; width: 100px; height: 32px; line-height: 32px}
    .container .int .block h3{ font-size: 1.75em; height: 70px}
    .container .int .block p{ font-size: 1.2em}
    /* .container .int,.container .pub_con, .ref_con, .part_con{ padding: 80px 0} */
    .container ul.fea li .box .tit{ font-size: 1.75em;}
    .container ul.fea li{ padding-right: 60px}
    .container .pub_con h4, .ref_con h4, .part_con h4{ font-size: 1.75em; font-weight: bold; line-height: 3em}
    .container .pub_con p, .ref_con p, .part_con p{ font-size: 1.5em; padding-bottom: 2em}
    .container .pub_con p span{ font-size: 1.2em}
    .container .contact h2 a,.contact .address h2,.layer .content h2, .layer .login h2{ font-size: 1.75em}
    .contact .address h2 a{ font-size: 1em}
    .container .team_touch input, .contact .send .btn-email, .ref_link{ font-size: 1em; height: 40px; line-height: 40px; width: 150px}
    .contact .send .email{ height: 32px}
    .layer .login ul li p span a,.container ul.team_text li p{ font-size: 1.2em}
    .container .fea_word p,.container ul.team_text li h2,.container .team_touch p,.container .team_banner h1{ font-size: 1.75em;    line-height: 1.75em;}
    .container ul.team_text li h2{ margin: 10px 0}
}

@media(max-width:940px){ .container .banner h1 {padding-top: 30px;}.container .banner .btn {padding-top: 40px;}}
@media(max-width:800px){ .container .banner h1 {font-size: 3em;} .container .banner h2{ font-size: 1.4em} .container .banner .txt{  font-size: 1em} .container .banner .btn {padding-top: 30px;} .header .main .btn_sl a{ font-size: 1em; width: 60px; height: 24px; line-height: 24px}}
@media(max-width:750px){
