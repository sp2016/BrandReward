@charset "utf-8";
/* CSS Document */
@font-face {font-family: 'Proxima Nova'; src: url('../fonts/proximanova-light-webfont.eot'); src: url('../fonts/proximanova-light-webfont.woff') format('woff'), url('../fonts/proximanova-light-webfont.ttf') format('truetype'), url('../fonts/proximanova-light-webfont.svg') format('svg');font-weight:200;font-style:normal;}
@font-face {font-family: 'Proxima Nova'; src: url('../fonts/proximanova-reg-webfont.eot'); src: url('../fonts/proximanova-reg-webfont.woff') format('woff'), url('../fonts/proximanova-reg-webfont.ttf') format('truetype'),   url('../fonts/proximanova-reg-webfont.svg') format('svg');font-weight:300;font-style:normal;}
@font-face {font-family: 'Proxima Nova';src: url('../fonts/proximanova-bold-webfont.eot');src: url('../fonts/proximanova-bold-webfont.woff') format('woff'), url('../fonts/proximanova-bold-webfont.ttf') format('truetype'), url('../fonts/proximanova-bold-webfont.svg') format('svg'); font-weight:400;font-style:normal;}
@font-face { font-family:'Segoe UI'; src:url('../fonts/segoeui_0.eot'); src:url('../fonts/SegoeUI.woff') format('woff'), url('../fonts/SegoeUI.ttf') format('truetype'); src:url('../fonts/SegoeUI.svg') format('svg'); font-style:normal;}
@font-face { font-family:'LucidaSans'; src:url('../fonts/LucidaSansRegular.eot'); src:url('../fonts/LucidaSans.woff') format('woff'), url('../fonts/LucidaSans.ttf') format('truetype'); src:url('../fonts/LucidaSans.svg') format('svg'); font-style:normal;}
@font-face {font-family: 'comic'; src:url('../fonts/comic.eot');src:url('../fonts/ComicSansMS.woff') format('woff'), url('../fonts/comic.ttf') format('truetype'),url('../fonts/ComicSansMS.svg') format('svg');}
@font-face {font-family: 'elegance'; src:url('../fonts/Rounded%20Elegance.eot'); src:url('../fonts/RoundedElegance-Regular.woff') format('woff'),url('../fonts/Rounded%20Elegance.ttf') format('truetype'),url('../fonts/RoundedElegance-Regular.svg') format('svg');}
*{	 padding:0; 
	margin:0; 
	-webkit-box-sizing:border-box;
 	-moz-box-sizing: border-box;
   	box-sizing: border-box;
    -webkit-transition: all .4s ease-out;
    -moz-transition: all .4s ease-out;
    -ms-transition: all .4s ease-out;
    -o-transition: all .4s ease-out;
    transition: all .4s ease-out;
	font-family: 'Proxima Nova'; font-weight:300;
}
ul,li{ list-style:none;}
a{ text-decoration:none; color:#60738d;}
a:hover{ color:#f79646;}
p{ color: #444;}
/*header start*/
.head{ width:100%; background-color:#fff; position:fixed; top:0; height:90px; padding-top:10px; box-shadow:0 1px 4px #e4e4e4;z-index: 999;}
.main{ max-width:1200px; margin:0 auto; overflow:hidden;}
.head a.logo{ width:208px; height:56px; float: left;}
.head a.logo img{width: 100%;}
.head ul.menu{ float: left; margin-top:34px;
    display: block; margin-left:5%;margin-right:2.5%}
.head ul.menu li{    display: inline-block;
    font-size: 1.2em;}
.head ul.menu li a{ padding:0 10px; font-weight:300;}
.head .main .btn_sl{margin-top:26px;}
.head .main .btn_sl a{
    width: 64px;
    height: 32px;
    line-height: 32px;
    text-align: center;
    font-size: 14px;
    display: inline-block;
	border-radius:3px;}
.head .main .btn_sl a.su {
    border: 1px solid #f79646;
    background: #f79646;
    color: #ffffff;
}
.head .main .btn_sl a.su:hover{ background-color: #f57e1c;}
.head .main .btn_sl a.lg { border: 1px solid #60738d; margin-left:10px;}
.head .main .btn_sl a.lg:hover{ border: 1px solid #f79646; }
/*header end*/
/*banner start*/
.banner{ width: 100%;
    max-height: 600px;
    overflow: hidden;
     margin-top: 90px;}
.banner img{ width:100%;}
.banner_text {
    position: absolute;
    width: 100%;
    z-index: 2;
    top: 0; color:#fff;
   text-align: center; padding-top:250px;
}
.banner h2 {
    font-size: 2.8em;}
.banner h5{ font-size: 1.4em; padding: 20px 30px 70px 30px; font-weight: 300; color: #e3f1fb;}
.banner .btn {
    width: 450px;
    margin: 0 auto;
}
.banner .btn a {
    font-size: 1.2em;
    display: block;
    width: 186px;
    height: 48px;
    line-height: 48px;
    text-align: center;
	border-radius:3px;
}
.banner .btn a.s {
    border: 1px solid #f79646;
    float: left;
    margin: 0 20px 0 0;
    background: #f79646;
    color: #ffffff;
}
.banner .btn a.s:hover{background-color: #f57e1c;box-shadow: 0 2px 10px #f57e1c;}
.banner .btn a.l:hover,.pub_btn a:nth-child(2):hover{ border: 1px solid #f79646; background-color:#F0F6FE; color: #f79646;}
.banner .btn a.l,.pub_btn a:nth-child(2) {
    border: 1px solid #ffffff;
    float: left;
    background: none;
    color: #ffffff;
}
/*banner end*/
/*features start*/
.features{overflow:hidden;position: relative; background-color: #F0F6FE; /* margin-top: 99px; */ }

.features img{ width: 100%; position: absolute; z-index: -1;}
.features .main{ padding:60px 0; }
.fea_display a i,.fea_l h4:before,.fea_new h4:before,.icon_link a,.about_br i,.help_banner form button::before{ background:url(../img/icons.png) 0 0 no-repeat; width:80px; height:80px; display:block; margin:0 auto;}
.fea_display{ float:left; width:33.33%;}
.fea_display a p{ text-align:center; font-size:1.5em; margin-top:40px; color: #60738d;;}
.fea_display a i.iec{ background-position:-105px 0;}
.fea_display a i.rtr{ background-position:-209px 0;}
.fea_display a:hover i.fia{ background-position:0 -110px;}
.fea_display a:hover i.iec{ background-position:-105px -110px;}
.fea_display a:hover i.rtr{ background-position:-209px -110px;}
.fea_display a:hover p{ color:#f79646;}
.spspan{
    width: 146px;
    text-align: right;
    font-size: 18px;
    height: 34px;
    line-height: 34px;
}
/*features end*/
/* fea_add start */
.fea_add{ padding: 60px 0; overflow: hidden; background-color: #fff;}
.fea_add .fea_l,.fea_add .fea_r{ width: 50%; float: left;}
.fea_add .fea_l .fea_pic{ padding-right: 50px;}
.fea_add .fea_r .fea_pic{ padding-left: 50px}
.fea_add img{width: 100%; margin-bottom: 15px;}
.fea_add h4{ font-size: 1.2em; position: relative; padding-left: 40px; color: #60738d;    font-weight: 500;
    line-height: 40px;}
.fea_add h4:before{ content: ""; position: absolute; width: 30px; height: 30px; left: 0;background-position: -373px -3px;}
.fea_add .fea_l h4:before{ background-position: -330px -3px;}
.fea_add p{ font-size: 18px;}
.publishers_one a,.brand_logo a,.fea_nadd,.pub_one_content a,.pub_two_content a,.pub_three_content a,.pub_four_content a,.pub_five_content a,.referra a,.advertiser_contact a{ border-radius: 3px; display: block; text-align: center; color: #fff; height: 48px; line-height: 48px; background-color: #f79646; width: 180px; font-size: 1.2em; margin: 60px auto 0;overflow-x:hidden; overflow-y:hidden;}
.fea_add a:hover,.footer_banner a:hover,.pub_one_content a:hover,.pub_two_content a:hover, .pub_three_content a:hover,.pub_five_content a:hover,.contact .form .form_list input.btn-email:hover, .referra a:hover{ background-color: #f57e1c}
.fea_button{ width: 100%; overflow: auto;}
/* fea_add end */
/* brand_logo start */
.brand_logo{ background-color: #f0f6fe; overflow: hidden; padding: 10px 0; position: relative;}
.brand_logo iframe{ margin: 0; border: none;}
.brand_logo i{ position: relative; width: 5%; display: inline-block; float: left; height: 70px; cursor: pointer;}
.brand_logo i:after,.brand_logo i:before{    content: "";
    position: absolute;
    width: 2px;
    height: 24px;
    background: #60738d;
    border-radius: 3px;
    -moz-transition: all .3s ease;
    -ms-transition: all .3s ease;
    -o-transition: all .3s ease;
    -webkit-transition: all .3s ease;
    transition: all .3s ease;
    top: 10px; left: 10px;
    -moz-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    -o-transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
}
.brand_logo i:before{ top: 26px; right:0px;
    -moz-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    -o-transform: rotate(45deg);
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
}
.brand_logo i.arrow_r{ -moz-transform:scaleX(-1);
    -webkit-transform:scaleX(-1);
    -o-transform:scaleX(-1);
    transform:scaleX(-1);
    /*IE*/
    filter:FlipH;}
.br_logo_box{ float: left; width: 90%;}
.br_logo_box ul{ overflow: hidden;}
.br_logo_box ul li{ float: left; width: 20%;}
.br_logo_box ul li img{ width: 100%; display: inline-block;}
.bx-wrapper .bx-pager.bx-default-pager a{background-color:#60738d !important;}
.bx-wrapper .bx-pager.bx-default-pager a:hover, .bx-wrapper .bx-pager.bx-default-pager a.active{ background-color: #f79646!important;}
/* brand_logo end */
/* footer start  */
footer .footer{ background-color: #324155; overflow: hidden; color: #7d95b6; padding: 40px 0;

/* position: fixed;

*/
}
.link,.icon_link{ text-align: center;}
.icon_link{ margin-top: 30px;}
.link a{ color: #b5c2d2;margin: 0 4px; font-weight: 300;}
.link a:hover{ text-decoration: underline;}
.icon_link a{ width: 30px; height: 30px; display: inline-block; margin-right: 30px;}
.footer .icon_link a.t{ background-position: -418px 0px;}
.footer .icon_link a.f{ background-position: -472px 0px;}
.footer .icon_link a.p{ background-position: -520px 0px;}
.footer .icon_link a.i{ background-position: -569px 0px;}
/* footer end  */

/* our_team start */
.banner_video{ width: 100%; max-height: 560px; overflow: hidden;}
.banner_video  img{ width: 100%;}
/* our_team end */
/* About Brandreward start */
.about_br { padding: 60px 0;padding-top: 140px; background-color: #f0f6fe;}
.about_br h3,.meet_team h3,.team_text h3,.contact h3,.advertiser_text h3,.advertiser_brand h3,.advertiser_method h3,.advertiser_in h3,.advertiser_contact h3{ font-size: 1.5em;/* font-family:'Segoe UI'; */ font-weight: 500; color: #627491; margin-bottom: 15px;}
.about_br i{ width: 70px; height: 60px; margin: 0; background-position: -310px -107px; display: inline-block; float: left;}
.about_br p, .team_text p,.advertiser_text p,.advertiser_brand p,.advertiser_method p,.advertiser_in p,.advertiser_contact p{ line-height: 1.4em; font-size: 1.2em; margin-bottom: 10px;}
.advertiser_contact p{ margin-bottom: 50px;}
/* About Brandreward end */
/*  Meet Our Team start */
.meet_team{ padding: 60px 0 20px;}
.meet_team ul{ overflow: hidden;}
.meet_team ul li{ width: 32%; margin-right: 2%; float: left; position: relative; margin-bottom: 30px; cursor: pointer;}
.meet_team ul li.us{ background-color: #7388a3; overflow: hidden; /* max-height: 390px; overflow: hidden; */}
.meet_team ul li img{ width: 100%;}
.meet_team ul li p.name{ padding: 10px 20px 15px 20px; font-size: 1em; font-weight: bold; color: #313338; background-color: #f0f6fe; width: 100%;}
.meet_team ul li p span{ color: #f88926; font-size: 1.2em; font-weight: 300; margin-bottom: 6px; display: block;}
.meet_team ul li:nth-child(3n){ margin-right: 0;}
.meet_team ul li .topic{ display: none; font-size: 1.2em; padding:20px 20px 0 20px; font-weight: 300;  height: 100%; background: rgba(96,115,141,.7); width: 100%; position: absolute; top: 0; left: 0; color: #fff; } 
.meet_team ul li .topic h6{ font-size: 18px; font-weight: 300; margin-bottom: 6px;}
.meet_team ul li .topic .b_pic{ overflow: hidden;}
.meet_team ul li .topic .b_pic p{ max-width: 150px;  float: left; margin-right: 15px; margin-bottom: 20px;}
.meet_team ul li .topic span{ margin-bottom: 20px; display: inline-block;font-family: 'comic'; font-weight: lighter; font-size: 15px; letter-spacing: .5px;}
 .meet_team ul li:hover p.name{ background-color: #f2f3ff;  } 
.meet_team ul li:hover .topic{ display: block;}

/*  Meet Our Team end */
 /* The Brandreward Team&Why Work With Us?  */
.team_text{ padding: 60px 0; background-color: #f0f6fe;}
.br_team{ margin-bottom: 80px;}
.br_team, .work_with{ overflow: hidden;}
.br_team video{ float: left; width: 40%; margin-right: 40px;}
.work_with img{ float: right; width: 40%; margin-left: 40px;}
.work_with h3, .work_with p{ text-align: right}
/* footer_banner start */
.footer_banner img{ width: 100%; position: absolute; top: 0; left: 0; z-index: -1;}
.footer_banner p{ padding-top: 0px; color: #fff; font-size: 1.5em; text-align: center;}
.footer_banner a,.contact .form .form_list input.btn-email,.faq_more a{ border-radius: 3px; width: 180px; height: 48px; line-height: 48px; background-color: #f79646; color: #fff; display: block; text-align: center; font-size: 1.2em; margin: 40px auto 0;}
/* footer_banner end */
/*  contact us start */
.contact_us_banner .banner_text,.features_banner .banner_text,.publishers_banner .banner_text{ padding-top: 200px;}
.features_banner .banner_text{ padding-top: 140px;}
.contact_us_banner .banner_text .main,.features_banner .banner_text .main,.publishers_banner .banner_text .main{ background: rgba(0,0,0,.3); padding: 20px 0; }
.contact_us_banner .banner_text h2{ font-weight: 500; color: #fff; }
.contact_us_banner .banner_text h5{ font-size: 1.5em; padding: 30px; font-weight: 300;}
.features_banner h5{ padding-bottom: 50px;}
.contact{ overflow: hidden; padding: 60px 0; background-color: #F0F6FE;}
.contact .form{ width: 55%; float: left;}
.contact .details{ width: 45%; float: left; padding-left: 60px; box-sizing:border-box;}
.contact .form .form_list label{ display: block; font-size: 1em; color: #333; margin-bottom: 4px;}
.contact .form .form_list{ margin-bottom: 20px;}
.contact .form .form_list span{ display: none;}
.contact .form .form_list input,.contact .form .form_list textarea,.contact .form .form_list select{ text-indent: 10px; width: 100%; height: 36px; line-height: 36px; border-radius: 3px; border: none; background-color: #f1f5f9;    border: 1px solid #E1E7EF;
    background-color: #fafdff;}
.contact .form .form_list textarea{ height: 200px; padding-left: 20px; text-indent: 0; line-height: 24px;}
.contact .form .form_list input.btn-email{ margin: 0; cursor: pointer; text-indent: 0;}
.contact .details>div h4,.features_display .main h4{ font-size: 1.2em;/*  font-family:'Segoe UI'; */ color: #333; font-weight: 500; }
.contact .details>div p{ font-size: 1.1em;  color: #000; margin: 10px 0 20px 0;}
.contact .details img{ width: 209px; margin-top: 30px;}
.contact .details .details_pic{ margin-top: 10px; width: 100%; max-height: 292px; overflow: hidden;}
.contact .details>div p:frist-child{ margin-bottom: 30px;}
/*  contact us end */
.min_menu{ display: none;}
/* features display start */
.features_display{ padding: 60px 0; background-color: #F0F6FE;}
.features_display .main>div,.features_display .main>div>ul{ overflow: hidden;}
.features_display .main>div>ul{ margin-bottom: 40px;}
.features_display .main>div>ul .li{ float: left; width: 47%; margin-right: 6%; padding-left: 60px; position: relative;}
.features_display .main>div>ul li{ float: left; width: 47%; margin-right: 6%; position: relative;}
.features_display .main>div>ul li p a{ color: #2086d9; /* color:#f79646; text-decoration: underline;   */}
.features_display .main>div>ul li p a:hover{ color: #2086d9; text-decoration: underline;}
.features_display .main>div>ul .li:before{ content: ""; width: 40px; height: 40px; position: absolute; left: 0; top: 0; background: url(../img/icons.png) 0 0 no-repeat;}
.features_display .main>div>ul li:nth-child(2n){ margin-right: 0;}
.features_display .main>div>ul li.fea_dis_b:before{ background-position: -5px -206px;}
.features_display .main>div>ul li.fea_dis_c:before{ background-position: -61px -206px;}
.features_display .main>div>ul li.fea_dis_d:before{ background-position: -121px -206px;}
.features_display .main>div>ul li.fea_dis_a:before{ background-position: -179px -206px;}
.features_display .main>div>ul li.fea_dis_e:before{ background-position: -231px -206px;}
.features_display .main>div>ul li.fea_dis_f:before{ background-position: -291px -206px;}
.features_display .main h4{ margin-bottom: 15px; color: #60738d; font-size: 1.5em;}
.features_display .main p{ font-size: 1.2em; line-height: 1.4em;}


/* features display end */
/* publishers start */
.publishers_banner,.advertiser_banner,.features_banner,.contact_us_banner,.referra_banner,.help_banner{  min-height: 300px;  position: relative;}
.publishers_banner h5{ font-size: 2.4em;}
.publishers_banner span,.advertiser_banner span,.features_banner span,.contact_us_banner span,.referra_banner span,.help_banner span{ background: url(../img/publishers_pic.jpg)  center no-repeat; width: 100%; height: 100%; position: absolute;}
.advertiser_banner span{ background: url(../img/advertiser_pic.jpg) center no-repeat;}
.features_banner span{ background: url(../img/features_pic.jpg) center no-repeat;}
.contact_us_banner span{ background:url(../img/contact_banner%20-%202.jpg) center no-repeat; }
.referra_banner span{ background:url(../img/referral_pic.jpg) center no-repeat; }
.help_banner span{ background:url(../img/help_pic.jpg) center no-repeat; }
.publishers_one h4,.publishers_two h4,.publishers_three h4,.publishers_four h4,.publishers_five h4{ color: #627491; font-size: 1.5em; /* font-family: 'Segoe UI'; */  font-weight: 500; }
.publishers_five h4,.pub_five_content p{ text-align: center;}
.publishers_one{ background-color: #F0F6FE; padding: 100px 0; overflow: hidden;}
.publishers_one img,.publishers_three img,.publishers_five img{ width: 35%; bottom: 0; float: left; margin-right: 20px;}
.publishers_three img { width: 35%;  margin-right: 0; }
 .publishers_five img{ width: 56%; float: inherit; margin: 0 auto; display: block;}
.pub_one_content p,.pub_three_content p,.pub_five_content p{ margin: 15px 0 50px 0; line-height: 1.4em; font-size: 1.2em;}
.pub_one_content p a,.pub_one_content p a:hover,.pub_two_content p a,.pub_two_content p a:hover,.pub_three_content p a,.pub_three_content p a:hover{ color: #2086d9; display: inline; background-color: rgba(0,0,0,0);   line-height: 1.4em;
  font-size: 1em;}
.pub_one_content p a:hover,.pub_two_content p a:hover,.pub_three_content p a:hover{ text-decoration: underline;}
.publishers_two{ width: 100%;position: relative; overflow: hidden; padding: 100px 0;}
.publishers_two img,.publishers_four img,.faq_more img,.referra img,.advertiser_brand img,.advertiser_in img{ width: 100%; position: absolute; top: 0; left: 0; z-index: -1;}
.publishers_two h4,.pub_two_content p,.advertiser_in h3{ color: #fff; text-align: center;}
.publishers_two h4{ font-size: 1.8em; font-weight:500;}
.pub_btn a{ float: left;}
.pub_btn a:first-child{ margin-right: 30px;}
.pub_btn{ overflow: hidden; margin: 0 auto;  width: 390px;}
.pub_two_content a,.pub_four_content a{ overflow: visible;}
.pub_two_content p,.advertiser_in p{ padding: 30px 0 0;  font-size: 1.2em;  color: #e2eeff; line-height: 1.4em;}
.publishers_three{ padding: 100px 0; background-color: #F0F6FE;}
.publishers_four h4{ text-align:center;}
.publishers_four h4 span{ font-weight: 300px;}
.publishers_four .pub_btn a:nth-child(2){border: 1px solid #60738d; color: #60738d}
.publishers_four .pub_btn a:nth-child(2):hover{ color: #fff;}
.publishers_five{ padding: 100px 0;}
/* publishers end */
/* help start */
.help_banner{ max-height: 300px;}
.help_banner .banner_text{ padding-top: 40px;}
.help_banner h5{ font-size: 2.2em; padding: 20px 30px 70px 30px;}
.help_banner input{width: 100%; height: 40px; padding-left: 20px; background-color: rgba(255,255,255,.9); font-size: 1em; border-radius: 3px; border: 1px solid #60738d; color: #60738d; }
.help_banner form button::before{ content: ""; width: 24px; height: 24px; position: absolute; top: 7px; right: 8px; background-position: -410px -43px; z-index: 4; display: inline-block;}
.help_banner form button{ cursor: pointer; position: absolute; border-radius: 0 3px 3px 0; background-color: #60738d; width: 48px; height: 38px; border: none; top: 1px; right: 1px;}
.help_banner form{ max-width: 400px; margin: 0 auto; position: relative; box-shadow: 0 4px 2px #000; }
.help_banner form:before{ content: ""; background: url(../img/search_dog.png) 0 0 no-repeat; width: 75px; height: 75px; position: absolute;top: -60px; left: 40%;}
.faq{ padding: 60px 0; background-color: #F0F6FE;}
.faq:nth-child(2n){ background-color: #fff;}
.faq h3{ text-align: center;  font-size: 1.8em; /* font-family: 'Segoe UI'; */ font-weight: 500; margin-bottom: 30px;color: #627491;}
.faq ul li p{ padding:0 20px; font-size: 1.2em;}
.faq ul li p a{  cursor: pointer;   color: #2086d9;}
.faq ul li p a:hover{text-decoration: underline;color: #2086d9; }
.faq ul li p.q{ background-color: #60738d; width: 100%; color: #fff; padding: 8px 0 8px 20px; margin-bottom: 15px;}
.faq ul li{ margin-bottom: 30px; background-color: #fff; padding-bottom: 30px;}
.faq:nth-child(2n) ul li{background-color: #F0F6FE;}   
.faq ul li:last-child{ margin-bottom: 0;}
.faq_more h4{  font-size: 1.5em;  color: #627491; width: 100%; text-align: center;}
.faq_more a,.footer_banner a,.publishers_four a{ margin-top: 30px; }
